{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/InstantMultiplayer/bin/Debug/netcoreapp3.1/InstantMultiplayer.dll",
      "args": [],
      "cwd": "${workspaceFolder}/InstantMultiplayer",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "Docker .NET Core Launch",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "netCore": {
        "appProject": "${workspaceFolder}/InstantMultiplayer/InstantMultiplayer.csproj"
      }
    }
  ]
}